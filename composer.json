{
    "name": "fob/formal-bears-demo",
    "type": "project",
    "license": "CC0-1.0",
    "require": {
        "php": ">=7.0.8",
        "bear/package": "^1.9",
        "josegonzalez/dotenv": "^3.2",
        "fob/formalbears": "^0.1.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "bear/qatools": "dev-master",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Fob\\FormalBearsDemo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fob\\FormalBearsDemo\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "php bin/setup.php"
        ],
        "compile": "bear.compile 'Fob\\FormalBearsDemo' prod-app ./"
    },
    "scripts-descriptions": {
        "setup": "Setup the project",
        "compile": "Compile autoload and DI scripts"
    },
    "config": {
        "process-timeout": 0
    }
}
